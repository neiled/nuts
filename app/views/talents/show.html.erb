<div class="container">
  <div class="page-header">
    <h1>
      <%= @talent.first_name %>
      <%= @talent.middle_name %>
      <%= @talent.last_name %>
    </h1>
  </div>

  <% for item in @talent.photos %>
      <%= raw get_thumb_image(item) %> 
  <% end %>

  <%= link_to "Edit", edit_talent_path(@talent), class: "btn btn-default" %>
  <%= link_to "Talent List", talents_path, class: "btn btn-primary" %>
</div>
